<odoo>
    <data noupdate="1">

        <record id="ba_sale_order" model="sale.order">
            <field name="partner_id" ref="base.res_partner_2"/>
            <field name="partner_invoice_id" ref="base.res_partner_2"/>
            <field name="partner_shipping_id" ref="base.res_partner_2"/>
            <field name="user_id" ref="base.user_demo"/>
            <field name="pricelist_id" ref="product.list0"/>
            <field name="team_id" ref="sales_team.team_sales_department"/>
            <field name="date_order" eval="(DateTime.today() - relativedelta(months=1)).strftime('%Y-%m-%d %H:%M')"/>
        </record>

        <record id="sale_order_line_ba" model="sale.order.line">
            <field name="order_id" ref="fenix_test_sale.ba_sale_order"/>
            <field name="name">Business Analysis</field>
            <field name="product_id" ref="fenix_test_sale.business_analysis"/>
            <field name="product_uom_qty">3</field>
            <field name="product_uom" ref="uom.product_uom_unit"/>
            <field name="price_unit">29500.00</field>
        </record>

        <record id="sale_order_line_odoo" model="sale.order.line">
            <field name="order_id" ref="fenix_test_sale.ba_sale_order"/>
            <field name="name">Business Analysis</field>
            <field name="product_id" ref="fenix_test_sale.odoo_implementation"/>
            <field name="product_uom_qty">3</field>
            <field name="product_uom" ref="uom.product_uom_unit"/>
            <field name="price_unit">29500.00</field>
        </record>


    </data>
</odoo>
